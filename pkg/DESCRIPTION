Package: seriation
Type: Package
Title: Infrastructure for Seriation
Version: 1.1-1
Date: 2015-7-30
Authors@R: c(
	person("Michael", "Hahsler", role = c("aut", "cre", "cph"),
	    email = "mhahsler@lyle.smu.edu"),
